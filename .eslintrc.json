{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "camelcase": "off",
      "max-classes-per-file": "off",
      "no-console": "off",
      "no-constructor-return": "off"
    }
}
